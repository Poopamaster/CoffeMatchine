<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.Image?>
<StackPane xmlns:fx="http://javafx.com/fxml" stylesheets="@/app.css">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20" />
    </padding>
    <VBox alignment="CENTER" spacing="10"  >
        <padding>
            <Insets top="20" right="20" bottom="20" left="20" />
        </padding>
        <ImageView fitWidth="100" fitHeight="100" >
            <Image url="@/coffee-cup.png"></Image>
        </ImageView>
        <StackPane styleClass="background">
            <Label fx:id="message" text=" baht has withdrawn from the machine." style="-fx-font-size: 24px;" />
        </StackPane>
            <Button maxWidth="Infinity"  fx:id="backButton" GridPane.rowIndex="1" GridPane.columnIndex="1">
                <graphic>
                    <VBox alignment="CENTER" spacing="5">
                        <HBox alignment="CENTER" spacing="25">
                            <ImageView fitWidth="50" fitHeight="50">
                                <image>
                                    <Image url="@/coffee-cup.png" />
                                </image>
                            </ImageView>
                            <Label text="Back" style="-fx-font-size: 24px;" />
                        </HBox>
                    </VBox>
                </graphic>
            </Button>


        <!-- Add other buttons with their icons similarly -->
    </VBox>
</StackPane>